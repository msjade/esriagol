{
  "auth": {
    "type": "generateToken",
    "portal": "https://www.arcgis.com",
    "username_env": "AGOL_USERNAME",
    "password_env": "AGOL_PASSWORD",
    "referer": "https://www.arcgis.com"
  },
  "services": {
    "ea_frame": {
      "feature_layer_query_url": "https://services.arcgis.com/XXXXXXX/ArcGIS/rest/services/EA_FRAME/FeatureServer/0/query",
      "vector_tile_base": "https://tiles.arcgis.com/tiles/XXXXXXX/arcgis/rest/services/EA_FRAME_VT/VectorTileServer",
      "allowed_out_fields": [
        "OBJECTID",
        "NAT_EA_SN",
        "STATE_NAME",
        "LGA_NAME",
        "WARD_NAME",
        "EA_NAME",
        "EA_ID",
        "URBAN_RURAL",
        "EST_POP"
      ]
    },

    "buildings": {
      "feature_layer_query_url": "https://services.arcgis.com/XXXXXXX/ArcGIS/rest/services/BUILDINGS/FeatureServer/0/query",
      "vector_tile_base": "https://tiles.arcgis.com/tiles/XXXXXXX/arcgis/rest/services/BUILDINGS_VT/VectorTileServer",
      "allowed_out_fields": [
        "OBJECTID",
        "BLD_ID",
        "NAT_EA_SN",
        "STATE_NAME",
        "LGA_NAME",
        "WARD_NAME",
        "EA_ID",
        "BLD_USE",
        "RES_STATUS"
      ]
    }
  }
}
